{
    "name": "makeitfly/cleantalk-symfony",
    "description": "Cleantalk anti-spam integration for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Gerarts",
            "email": "mark.gerarts@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MakeItFly\\CleanTalkBundle\\": "src/"
